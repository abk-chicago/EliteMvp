<!-- Check for more: https://gist.github.com/adavis/4ec286b8ac4204a74d51-->
<FindBugsFilter>
    <!-- Ignore issue: all issues in resource generation (R file)-->
    <Match>
        <Class name="~.*\.R\$.*"/>
    </Match>

    <!-- Ignore issue: in manifest files-->
    <Match>
        <Class name="~.*\.Manifest\$.*"/>
    </Match>

    <!-- Ignore issue: classes generated by AndroidAnnotations-->
    <Match>
        <Class name="~.*_"/>
    </Match>

    <!-- Ignore issue: setter methods of Enums must be removed or package private-->
    <Match>
        <Bug pattern="ME_ENUM_FIELD_SETTER"/>
    </Match>

</FindBugsFilter>